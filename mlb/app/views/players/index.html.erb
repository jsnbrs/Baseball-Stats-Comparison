<h1>Players</h1>

  <%= form_tag(players_path, :method => "get", id: "age-form") do %>
    <%= text_field_tag :age, params[:age], placeholder: "Age", id: "age" %>
    <%= text_field_tag :search1, params[:player1], placeholder: "Search Players", id: "player1" %>
    <%= text_field_tag :search2, params[:player2], placeholder: "Search Players", id: "player2" %>
  <%= submit_tag "Search", :name => nil, id: "submit" %>

  <% end %>